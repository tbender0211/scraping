{{#each scrape}}
    <div class="row">
        <div class="col s12 m6" style="margin-left: 25%">
        <div class="card blue-grey darken-1" data-id="{{this._id}}">
            <div class="card-content white-text">
            <span class="card-title">{{this.title}}</span>
            <p>{{this.summary}}</p>
            </div>
            <div class="card-action">
            <a href="{{this.link}}" target="_blank">Take a listen!</a>
            <a class="btn-floating btn-small waves-effect waves-light orange accent-2" id="save" data-id="{{this._id}}"><i class="material-icons">add</i></a>
            </div>
        </div>
        </div>
    </div>
{{/each}}
            